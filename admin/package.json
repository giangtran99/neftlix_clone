{
  "name": "backend-web-eyemiru",
  "version": "0.1.0",
  "description": "board application with Moleculer microservices",
  "scripts": {
    "dev": "moleculer-runner --env --repl --hot backend/services",
    "start": "cross-env NODE_ENV=production moleculer-runner --envfile .env.prod backend/services",
    "now-start": "cross-env NODE_ENV=production moleculer-runner --env backend/services",
    "cli": "moleculer connect --ns eyemiru-prod nats://localhost:4222",
    "build": "node webpack/build.js",
    "now-build": "echo \"Skip build\"",
    "lint": "eslint backend && vue-cli-service lint",
    "dc:up": "docker-compose up --build -d",
    "dc:down": "docker-compose down"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
